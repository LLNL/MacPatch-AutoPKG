<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Use this a shared recipe processor. Use it to upload packages to a MacPatch server.
To use this processor, set the 'Processor' key in your recipe to 'gov.llnl.autopkg.recipes.MacPatchImporterProcessor/MacPatchImporterProcessor'</string>


    <key>Identifier</key>
    <string>com.github.smsg-mac-dev.MacPatchImporterProcessor</string>

    <key>Input</key>
    <dict>
        <key>auth_user</key>
        <string>override_this</string>
        
        <key>auth_pass</key>
        <string>override_this</string>
        
        <key>mp_server_url</key>
        <string>override_this</string>
        
        <key>verify_ssl</key>
        <false/>
    </dict>
    
    <key>MinimumVersion</key>
    <string>0.4.0</string>

    <key>Process</key>
    <dict/>
</dict>
</plist>